#include <iostream>
using namespace std;
/*
A cashier distributes change using the maximum number of five dollar bills, 
followed by one dollar bills. For example, 19 yields 3 fives and 4 ones. Write a 
single statement that assigns the number of 1 dollar bills to variable numOnes, given 
amountToChange.
*/

int main() {
   int amountToChange;
   int numFives;
   int numOnes;

   cin >> amountToChange;
   numFives = amountToChange / 5;

   numOnes = amountToChange % 5; //Number of ones is Total Change Modulo 5, the remainder of change/$5

   cout << "numFives: " << numFives << endl;
   cout << "numOnes: " << numOnes << endl;

   return 0;
}
